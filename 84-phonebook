public Map<String, String> phonebook(List<Contact> contacts) {
    Map<Contact, String> nameAndPhone = new Map<Contact, String>();
    String p = '';

    for(Contact c : contacts) {
        if(p != null){
            nameAndPhone.get(p);
            nameAndPhone.put(c, p);
        }
        
        return nameAndPhone;
    }
}