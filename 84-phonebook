public Map<String, String> phonebook(List<Contact> contacts) {
    Map<String, String> nameAndPhone = new Map<String, String>([SELECT Phone FROM Contact WHERE Phone != null]);
    String contact = JSON.serialize(contacts);
    
        if(Phone != null) {
            nameAndPhone.get(Phone);
            nameAndPhone.put(contact, phoneNum);
        }
    
    return nameAndPhone;
}