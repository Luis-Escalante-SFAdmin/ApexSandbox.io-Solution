public Map<String, String> phonebook(List<Contact> contacts) {
    String phone;
    Map<String, String> newMap = new Map<String, String>();

    for(Contact c : contacts) {
        if(phone != null) {
            //newMap.get(c.phone);
            newMap.put(c.Name, c.phone);
        }
    }

    return newMap;
}