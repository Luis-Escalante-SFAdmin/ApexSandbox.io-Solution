public Map<String, String> phonebook(List<Contact> contacts) {
    Map<String, String> nameAndPhone = new Map<String, String>();
    String phone;
    
    for(Contact con : contacts){
        if(phone == null) {
            nameAndPhone.get(phone);
            //nameAndPhone.put(phone);
        } else {
            return nameAndPhone.put(con, phone);
        }
        }
    
    return nameAndPhone;
}