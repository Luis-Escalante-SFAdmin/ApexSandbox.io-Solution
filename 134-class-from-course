public Id classFromCourse(String courseName) {
    apxio__Class__c classes = [SELECT Id, (SELECT Id, Name FROM apxio__Course__c WHERE Name = :courseName) FROM apxio__Class__c];

    apxio__Course__c[] courses = new apxio__Course__c[](classes);
    classes.Id = apxio__Course__c.Id;

    insert classes;
    return Id;
}