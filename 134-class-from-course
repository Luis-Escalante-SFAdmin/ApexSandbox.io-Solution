public Id classFromCourse(String courseName) {
    apxio__Class__c classes = [SELECT Id, Name, apxio__Class__c FROM apxio__Course__c WHERE Name = apxio__Course__c.courseName];

    List<apxio__Course__c> courses = new List<apxio__Course__c>(Name);
    apxio__Course__c.Id = apxio__Class__c.Id;

    insert classes;
    return Id;
}