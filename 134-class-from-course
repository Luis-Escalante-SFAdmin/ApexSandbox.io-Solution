public Id classFromCourse(String courseName) {
    apxio__Course__c c = new apxio__Course__c();
    apxio__Course__c courses = [SELECT Id, Name FROM apxio__Course__c WHERE Name = :[SELECT Name FROM apxio__Course__c WHERE Id = :c.Id].Name];
    

    List<apxio__Class__c> cls = new List<apxio__Class__c>();
    


    insert cls;
    return courses.Id;
}