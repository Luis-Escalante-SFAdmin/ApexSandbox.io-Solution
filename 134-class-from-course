public Id classFromCourse(String courseName) {
    apxio__Class__c classes = [SELECT Id, Name, apxio__Course__c FROM apxio__Class__c WHERE Name = courseName];

    List<apxio__Course__c> courses = new List<apxio__Course__c>(Name);
    classes.Id = apxio__Course__c.Id;

    insert courses;
    return Id;
}