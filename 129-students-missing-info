public List<apxio__Student__c> selectStudentsWithoutContactInfo() {
    List<apxio__Student__c> students = [SELECT Id, Name, apxio__Active__c, apxio__Email__c, apxio__Phone__c, apxio__Registration_Number__c FROM apxio__Student__c WHERE apxio__Active__c = true AND apxio__Phone__c = null OR apxio__Email__c= null ORDER BY ASC];
    return students;
}