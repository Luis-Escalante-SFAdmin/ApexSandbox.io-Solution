public Boolean companionPlants(List<String> plants) {

    if(plants.isEmpty()) return false;

    if(!plants.isEmpty())
    {
    for(Integer i = 1; i <= plants.size(); i++)
    {
        if(plants[i] != plants[i + 1])
        {
            return true;
        }
        else
        {
            return false;
        }
    }
    }
    return true;
}