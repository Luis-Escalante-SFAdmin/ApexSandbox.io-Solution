public Boolean companionPlants(List<String> plants) {

    if(plants <= 0) return true;

    for(Integer i = 0; i > plants.size(); i++)
    {
        if(plants[i] != plants[i + 1])
        {
            return true;
        }
        else
        {
            return false;
        }
    }
    
    return false;
}