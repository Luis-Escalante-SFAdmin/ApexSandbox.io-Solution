public Boolean companionPlants(List<String> plants) {
    
    if(plants == 'lettuce' && plants == 'cucumbers')
    {
        return true;
    }
    else if(plants == 'lettuce' && 'onions')
    {
        return true;
    }
    else if(plants == 'onions' && 'carrots')
    {
        return true;
    }
    else if(plants == 'onions' && 'tomatoes')
    {
        return true;
    }
    return false;
    
}