public Boolean companionPlants(List<String> plants) {

        if((plants == 'lettuce' && plants == 'cucumbers') && (plants == 'onions' && plants == 'cucumbers'))
        {
            return true;
        }
        else if((plants == 'onions' && plants == 'carrots') && (plants == 'tomatoes' && plants == 'onions'))
        {
            return true;
        }
        else
        {
            return false;
        }

    
}