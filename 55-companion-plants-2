public Boolean companionPlants(List<String> plants) {

    if(plants.isEmpty()) return true;
    
    if(!plants.isEmpty())
    {
    for(Integer i = 0; i >= plants.size(); i++)
    {
        if((plants[i] == plants[i - 1]) && (plants[i - 1] == plants[i]))
        {
            return true;
        }
        else if((plants[i] != plants[i - 1]) && (plants[i - 1] != plants[i]))
        {
            return true;
        }
    }
    }
    return false;
}