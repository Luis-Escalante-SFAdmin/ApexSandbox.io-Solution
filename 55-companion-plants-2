public Boolean companionPlants(List<String> plants) {

    if(plants.size() <= 1) return true;
    
    if(plants.size() >= 1)
    {
    for(Integer i = 0; i < plants.size() - 1; i++)
    {
        if(plants[i] != plants[i])
        {
            return true;
        }
    }
    }
    return false;
}