public Boolean companionPlants(List<String> plants) {

    if(plants == null) return false;

    for(String plant : plants)
    {
        if((plant == 'lettuce' && plant == 'cucumbers') && (plant == 'onions' && plant == 'cucumbers'))
        {
            return true;
        }
        else if((plant == 'onions' && plant == 'carrots') && (plant == 'tomatoes' && plant == 'onions'))
        {
            return true;
        }
    }
    return false;
}