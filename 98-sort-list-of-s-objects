public List<Account> getAccounts(List<Account> accounts) {

    List<AccountWrapper> accWrapper = new List<AccountWrapper>();   

        for(Account a : accounts)
        {
            AccountWrapper wrapperAcc = new AccountWrapper(a);
            accWrapper.add(wrapperAcc);
        }
    accWrapper.sort(Comparator.comparing(AccountWrapper::getAccountAnnualRevenue));
    return accWrapper;
}
// write extra class here 
public class AccountWrapper implements Comparable
{
    public Account acc;

    public Integer compareTo(Object compareTo)
    {
        AccountWrapper otherAcc = (AccountWrapper)compareTo;

        Integer returnValue = 0;
        if((acc.AnnualRevenue == null) && (otherAcc.acc.AnnualRevenue == null))
        {
            returnValue = 0;
        }
        else if((acc.AnnualRevenue == null) && (otherAcc.acc.AnnualRevenue != null))
        {
            returnValue = -1;
        }
        else if((acc.AnnualRevenue != null) && (otherAcc.acc.AnnualRevenue == null))
        {
            returnValue = 1;
        }
        else if(acc.AnnualRevenue > otherAcc.acc.AnnualRevenue)
        {
            returnValue = 1;
        }
        else if(acc.AnnualRevenue < otherAcc.acc.AnnualRevenue)
        {
            returnValue = -1;
        }
        return returnValue;
    }
}