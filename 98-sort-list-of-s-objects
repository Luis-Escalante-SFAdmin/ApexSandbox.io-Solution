public List<Account> getAccounts(List<Account> accounts) {

List<AccountWrapper> accWrapper = new List<AccountWrapper>();
    
    for(Account a : accounts)
    {
        accWrapper.add(new AccountWrapper(a));       
    }
    accWrapper.sort();

    List<Account> sortedAcc = new List<Account>();

    for(AccountWrapper aw : accWrapper)
    {
        sortedAcc.add(aw.acc);
    }
    return sortedAcc;
}
// write extra class here 
public class AccountWrapper implements Comparable
{
    Account acc = new Account();
    String Name;
    Integer AnnualRevenue;

    public AccountWrapper(Account act)
    {
        acc = act;
        Name = act.Name;
        AnnualRevenue = act.AnnualRevenue;
    }

    public Integer compareTo(Object obj)
    {
        AccountWrapper otherAcc = (AccountWrapper)obj;

        if(AnnualRevenue == otherAcc.AnnualRevenue)
        {
          return 0;
        }
        else if(AnnualRevenue < otherAcc.AnnualRevenue)
        {
            return 1;
        }
        else
        {
        return -1;
        }
    }
}