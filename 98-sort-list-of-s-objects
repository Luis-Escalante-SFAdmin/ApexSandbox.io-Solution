public List<Account> getAccounts(List<Account> accounts) {

List<AccountWrapper> accWrapper = new List<AccountWrapper>();
    
    for(AccountWrapper aWrap : accWrapper)
    {
        Account account = new Account();

        account.Name = aWrap.acc.Name;
        account.AnnualRevenue = aWrap.acc.AnnualRevenue;
        aWrap.add(account);
        
    }
    accounts.sort();
    return accounts;
}
// write extra class here 
public class AccountWrapper implements Comparable
{
    public Account acc;
    public Integer AnnualRevenue;

    public Integer compareTo(Object obj)
    {
        AccountWrapper otherAcc = (AccountWrapper)obj;

        if(this.AnnualRevenue == otherAcc.AnnualRevenue)
        {
          return 0;
        }
        else if(this.AnnualRevenue > otherAcc.AnnualRevenue)
        {
            return 1;
        }
        return -1;
    }

}