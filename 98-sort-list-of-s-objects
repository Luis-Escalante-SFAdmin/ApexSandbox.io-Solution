public List<Account> getAccounts(List<Account> accounts) {

List<AccountWrapper> accWrapper = new List<AccountWrapper>();
    
    for(Account a : [SELECT Name, AnnualRevenue FROM Account])
    {
        accWrapper.add(new accWrapper(a.Name, a.AnnualRevenue));
    }
    if(accWrapper != null && accWrapper.size() > 0)
    {
        accWrapper.sort();
    }
    return accWrapper;
}
// write extra class here 
public class AccountWrapper implements Comparable
{
    public Account acc;
    public Integer AnnualRevenue;

    public Integer compareTo(Object compareTo)
    {
        AccountWrapper otherAcc = (AccountWrapper)compareTo;

        Integer returnValue = 0;
        if(AnnualRevenue > otherAcc.AnnualRevenue)
        {
          returnValue = 1;
        }
        else if(AnnualRevenue < otherAcc.AnnualRevenue)
        {
            returnValue = -1;
        }
        return returnValue;
    }

}