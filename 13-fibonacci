public Integer fibonacci(Integer n) {
    List<Integer> fibSeq = new List<Integer>();

    if(n == 0){
        fibSeq.add(0);
    }
    if(n >= 1){
        fibSeq.add(1);
    }

    for(Integer i = 2; i < n; i++){
        Integer nextNum = fibSeq[i - 1] + fibSeq[i - 2];
        fibSeq.add(nextNum);
    }

    return n;
}