public String employeeEmail(Map<String, String> employeeIdToEmail, String employeeId) {
    String defaultEmail = 'info@apexsandbox.io';
    employeeIdToEmail.put(employeeId, '');

    if(employeeIdToEmail.containsKey(employeeId)) {
        employeeIdToEmail.get(defaultEmail);
        return defaultEmail;
    } else {
        return defaultEmail;
    }
        
}