public String employeeEmail(Map<String, String> employeeIdToEmail, String employeeId) {
    String defaultEmail = 'info@apexsandbox.io';

    employeeIdToEmail.put(employeeId, defaultEmail);
    
    if(employeeIdToEmail.containsKey('info@apexsandbox.io')) {
        return defaultEmail;

    } else {
        return employeeIdToEmail.get(employeeId);
    }

}