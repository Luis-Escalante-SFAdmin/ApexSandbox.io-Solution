public String employeeEmail(Map<String, String> employeeIdToEmail, String employeeId) {
    String defaultEmail = 'info@apexsandbox.io';

    Boolean contains = employeeIdToEmail.containsKey(employeeId);

    if(contains == true) {
        employeeIdToEmail.get(employeeId);
        return employeeId;
    } else {
        return defaultEmail;
    }
        
}