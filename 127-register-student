public String registerStudent(String name, String phone, String email) {
    Number apxio__Registration_Number__c;
    apxio__Student__c newRegistration = new apxio__Student__c(Name= name, apxio__Phone__c= phone, apxio__Email__c= email);
    insert newRegistration;
    List<apxio__Student__c> studentsNumber = [SELECT Name, Id, apxio__Phone__c, apxio__Email__c FROM apxio__Student__c];
    return studentsNumber.apxio__Registration_Number__c;
}