public String registerStudent(String name, String phone, String email) {
    apxio__Student__c newRegistration = new apxio__Student__c();
    insert newRegistration;
    newRegistration = [SELECT Name, apxio__Registration_Number__c, apxio__Phone__c, apxio__Email__c FROM apxio__Student__c LIMIT 1];
    return newRegistration.apxio__Registration_Number__c;
}
