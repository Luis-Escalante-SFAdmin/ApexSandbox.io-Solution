public Set<Id> accountIds(List<Opportunity> opps) {
    Set<Id> accId = new Set<Id>();

    for(Opportunity opp : opps) {
       Opportunity o = [SELECT Id, Name FROM Opportunity WHERE Id =: accId LIMIT 1];
       if(o != null)
        opp = o.Id;
        accId.add(opp);
    }
    
    return accId;
}