public Set<Id> accountIds(List<Opportunity> opps) {
    Set<Id> accId = new Set<Id>();
    
    Account[] a = new Account[]{};
    for(Opportunity opp : opps) {
       Opportunity o = [SELECT Id, Name FROM Opportunity LIMIT 1];
           
        if(opps.AccountId == Account.Id) {
            a.add(opp);
        }
       
    }
    return accId;
}