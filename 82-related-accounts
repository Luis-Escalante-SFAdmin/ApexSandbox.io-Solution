public Set<Id> accountIds(List<Opportunity> opps) {
    Set<Id> accId = new Set<Id>();
    
    Account[] a = new Account[]{};
    for(Opportunity opp : opps) {
       Account acc = [SELECT Id, Name, (SELECT AccountId FROM Opportunities) FROM Account WHERE Id = :AccountId LIMIT 1];
           
           o.add(opp);
       
    }
    
    return accId;
}