public Boolean isKeyAccount(Account a) {
    if(a.AnnualRevenue == 600000)
    {
        a.Industry == 'Banking';
        return true;
    }
    else if(a.AnnualRevenue == 800000)
    {
        a == 'Technology';
        return true;
    }
    else if(a.AnnualRevenue == 2000000)
    {
        a == 'Retail';
        return true;
    }
    else if(a.AnnualRevenue == 500000)
    {
        a == 'All others';
        return true;
    }
    return true;
}