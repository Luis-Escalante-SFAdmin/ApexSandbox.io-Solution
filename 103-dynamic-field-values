public List<String> getFieldsValue(Account acc, List<String> fields)
{
    Account accounts = new Account(String Name, String BillingCity, String AnnualRevenue);
    String name = (String)accounts.get('Name');
    String bCity = (String)accounts.get('BillingCity');
    String revenue = (String)accounts.get('AnnualRevenue');

    List<String> newFields = new List<String>(name, bCity, revenue);

    String strFields = String.valueOf(newFields);

    return strFields;

}