public List<String> getFieldsValue(Account acc, List<String> fields)
{

    List<String> results = new List<String>();

    for(String field : fields)
    {
        if(field != null)
        {
            field = (String)acc.get('fields');
            String values = String.valueOf(field);
            results.add(values);
           
        }
        
    }

    return results;

}