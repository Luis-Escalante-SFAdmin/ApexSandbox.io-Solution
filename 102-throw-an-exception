public List<Account> checkAccounts(List<Account> accounts)
{
    List<Account> acc = new List<Account>();
    String BillingCity;
    String Name;
    acc.add(new Account());

    try
    {

        if(BillingCity == null) throw new IllegalArgumentException('accounts should not be null');

        insert acc;
        
    }
    catch(AccountException e)
    {
        throw new AccountException('Invalid BillingCity');
    }

    return accounts;

} 

//do not remove the following custom-defined exception
public class AccountException extends Exception {}