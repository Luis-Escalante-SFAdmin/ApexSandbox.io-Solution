public void insertStudents(List<String> studentNames, List<String> studentEmails) {
    List<apxio__Student__c> students = [SELECT Id, Name, apxio__Email__c FROM apxio__Student__c WHERE Name=: studentNames AND apxio__Email__c=: studentEmails LIMIT 50];
    for(apxio__Student__c std : students){
        System.debug(std);
    }

    Integer numStudents = students.size();
    for(Integer i = 0; i < numStudents; i++){
        System.debug(numStudents);
    }

    apxio__Student__c s = new apxio__Student__c();
    s.Name = studentNames;
    t.apxio__Email__c = studentEmails;
    students.add(s);
    insert students;
}