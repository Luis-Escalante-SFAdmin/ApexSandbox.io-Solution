public Integer secondLargest(List<Integer> numbers) {
    Integer maxValue = Integer.MIN_VALUE;
    Integer minValue = Integer.MIN_VALUE;

    for(Integer num : numbers){
        if(num > maxValue){
            minValue = maxValue;
            maxValue = num;
        }
        else if(num > minValue && num > maxValue){
            minValue = num;
        }
    }
    return maxValue;
}