public Integer secondLargest(List<Integer> numbers) {
    numbers.sort();
    Integer maxValue = numbers[0];
    Integer minValue = numbers[1];

    for(Integer num : numbers){
        for(Integer i = 1; i < numbers.size(); i++){
        if(numbers[i] < maxValue){
            return maxValue;
        }
        else if(numbers[i] > minValue){
            return minValue;
        }
        }
    }
    return maxValue;
}