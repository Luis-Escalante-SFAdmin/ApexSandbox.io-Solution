public Integer secondLargest(List<Integer> numbers) {
    Integer maxValue = -2147483647 - 1;
    Integer minValue = -2147483647 - 1;

    for(Integer num : numbers){
        if(num > maxValue){
            minValue = maxValue;
            maxValue = num;
        }
        else if(num > minValue && num > maxValue){
            minValue = num;
        }
    }
    return maxValue;
}