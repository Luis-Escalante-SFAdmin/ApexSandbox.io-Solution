public Integer secondLargest(List<Integer> numbers) {
    Integer min = -2147483647 - 1;
    Integer maxValue = min;
    Integer minValue = min;

    for(Integer num : numbers){
        if(num > maxValue){
            minValue = maxValue;
            maxValue = num;
        }
        else if(num > minValue && num < maxValue){
            minValue = num;
        }
    }
    return maxValue;
}