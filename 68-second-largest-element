public Integer secondLargest(List<Integer> numbers) {
    Integer secondHigh = numbers[0];
    Integer maxValue = secondHigh;
    Integer minValue = secondHigh;

    for(Integer j = 0; j < numbers.size(); j++){
        if(numbers[j] > secondHigh){
            secondHigh = numbers[j];
        }
    }

        for(Integer i = 0; i < numbers.size(); i++){
        if(numbers[i] > minValue){
            minValue = maxValue;
            minValue = numbers[i];
        }
        else if(numbers[i] > maxValue){
            maxValue = numbers[i];
        }
        }
    return minValue;
}