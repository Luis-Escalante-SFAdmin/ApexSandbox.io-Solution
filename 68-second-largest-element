public Integer secondLargest(List<Integer> numbers) {
    Integer l = -2147483647;
    Integer m = -2147483647 - 1;

    if(Integer i = 0; i < numbers.size(); i++){
        if(numbers > l){
            return l;
        }
        else if(numbers < m){
            return m;
        }
    }
    return m;
}