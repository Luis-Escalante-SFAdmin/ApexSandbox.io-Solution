public Boolean containsDuplicates(List<Integer> nums) {
    Set<Integer> result = new Set<Integer>();

    result.addAll(nums);

    for(Integer n = 0; n < nums.size() + n; n++) {
        if(result.contains(n)){
            result.add(n);
            return true;
        }
        else if(!result.contains(n < 1)) {
            return false;
        }
    }
    return false;
}