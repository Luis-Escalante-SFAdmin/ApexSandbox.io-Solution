public Boolean containsDuplicates(List<Integer> nums) {
    Set<Integer> result = new Set<Integer>();

    result.addAll(nums);

    if(result.isEmpty()) return false;
    
    for(Integer n : nums) {
        if(result.contains(n)){
            return true;
        }
        else if(!result.contains(n) || result.size() == 1){
            return false;
        }
        else if(result.size > 1 && !result.contains(n)) {
            return false;
        }
    }
    return true;
}