public Boolean containsDuplicates(List<Integer> nums) {
    Set<Integer> result = new Set<Integer>();

    result.addAll(nums);

    if((result.isEmpty() || result.size() == 1)) return false;
    
    for(Integer n : nums) {
        if(!result.contains(n) || result.isEmpty()){
            return false;
        }
        else if(result.contains(n)){
            return true;
        }
    }
    return true;
}