public Boolean containsDuplicates(List<Integer> nums) {
    Set<Integer> result = new Set<Integer>();

    result.addAll(nums);

    for(Integer n = 0; n < result; n++) {
        if(result.add(n)){
            return true;
        }
        else if(!result.add(n)) {
            return false;
        }
    }
    return true;
}