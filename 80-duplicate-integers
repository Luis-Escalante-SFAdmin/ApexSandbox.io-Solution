public Boolean containsDuplicates(List<Integer> nums) {
    Set<Integer> result = new Set<Integer>();

    result.addAll(nums);

    if(!result.isEmpty()) return true;
    
    for(Integer n : nums) {
        if(result.contains(n)){
            return true;
        }
        else if(result.isEmpty() || result.size() >= 1){
            return false;
        }
    }
    return false;
}