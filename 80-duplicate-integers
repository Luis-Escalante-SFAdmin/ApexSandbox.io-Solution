public Boolean containsDuplicates(List<Integer> nums) {
    Set<Integer> result = new Set<Integer>();

    result.addAll(nums);

    if(result.isEmpty()) return false;
    
    for(Integer n = 0; n < result.size(); n++) {
        if(result.contains(n)){
            return true;
        }
        else if(result.size() < n - 1){
            return false;
        }
    }
    return true;
}