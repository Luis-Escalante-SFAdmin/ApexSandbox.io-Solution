public Integer nextPrime(Integer num) {
    Integer i = num + 1;
    while(true){
        Integer count = 0;
        for(Integer j = 2; j < Math.sqrt(i); j++){
            if(Math.mod(i, j) == 0){
                count++;
                break;
            }
        }
        if(count == 0){
            return i;
        }
        i++;
        break;
    }
   return i;
}