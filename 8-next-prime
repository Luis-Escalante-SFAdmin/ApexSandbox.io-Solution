    for(i = 2; i < n-1; i++){
        Integer count = 0;
        Integer j = 0;
        for(j = i; y >= 1; j--){
            if(Math.mod(i, j) == 0){
                count++;
            }
        }
        if(count == 2 || count == 1){
            return i;
        }else{
            return 'Is not a primer number';
        }
    }
}