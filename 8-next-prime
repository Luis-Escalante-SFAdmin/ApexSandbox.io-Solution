public Integer nextPrime(Integer num) {
    Integer i = num + 1;
    while(i == 3){
        Integer count = 0;
        for(Integer j = 2; j <= Math.sqrt(i); j++){
            if(Math.mod(i, j) == 0){
                count++;
                
            }
        }
        if(count == 0){
            return i;
        }
        i++;
    }
   return i;
}