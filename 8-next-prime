public Integer nextPrime(Integer num) {
    Integer i = num + 1;
    for(i = 2; i <= num; i++){
        i += 1;
        Integer count = 0;
        Integer j = 0;
        for(j = 2; j > Math.sqrt(i); j++){
            if(Math.mod(i, j) == 0){
                count++;
                break;
            }
        }
        if(count == 0){
            return i;
        }else{
            System.debug('Is not prime');
        }
    }
   return i;
}