public Set<Integer> setIntersection(Set<Integer> set1, Set<Integer> set2) {
    Set<Integer> set3 = new Set<Integer>();

    if(set1 == null) return null;
    for(Integer i = 0; i < set1.size(); i++){
        if(set1[0] == set2){
            set1.addAll(set2);
            return set1;
        }
        else if(set2 == set1){
            return set2;
        }
    }
    return set3;
}