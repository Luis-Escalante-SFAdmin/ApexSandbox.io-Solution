public Set<Integer> setIntersection(Set<Integer> set1, Set<Integer> set2) {
    Set<Integer> set3 = new Set<Integer>();

    if(set1.isEmpty()) return null;
    for(Integer i = 0; i < set1.size(); i++){
        if(!set2.isEmpty() && !set1.isEmpty() && set1.containsAll(set2)){
            set1.addAll(set2);
            return set1;
        }
    }
    set3.addAll(set1);
    return set3;
}