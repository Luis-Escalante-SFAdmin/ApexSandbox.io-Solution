public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
    List<String> fN = new List<String>();

    for(Integer i = 0; i < firstNames.size(); i++)
    {
        fN.add(firstNames);
    }
    for(Integer j = 0; j < lastNames.size(); j++)
    {
        fN.add(lastNames);
    }

    return fN[i] + fN[j];
}