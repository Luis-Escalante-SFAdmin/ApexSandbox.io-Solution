public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
    List<String> fN = new List<String>();

    for(Integer i = 0; i < firstNames.size() && i < lastNames.size(); i++)
    {
       String concatenated = firstNames[i] + lastNames[i];
       fN.add(concatenated);
    }
    return fN;
    
}