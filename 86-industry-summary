public Map<String, Decimal> industrySummary(List<Account> accounts) {
    Map<String, Decimal> result = new Map<String, Decimal>();
    Set<Account> nSet = new Set<Account>();

    nSet.addAll(accounts);

    for(Account acc : nSet) {
        if(result.containsKey(nSet)) {
            result.put(nSet, acc.AnnualRevenue);
        }
    }
    return result;
}
