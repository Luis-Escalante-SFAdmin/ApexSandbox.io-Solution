public Map<String, Decimal> industrySummary(List<Account> accounts) {
    Map<String, Decimal> resuly = new Map<String, Decimal>();

    for(Account acc : accounts) {
        Map<String, Decimal> newMap = new Map<String, Decimal>{acc.Industry => acc.AnnualRevenue};
        if(newMap.containsKey(acc.Industry)) {
            newMap.put(acc.Industry, acc.AnnualRevenue);
        }
    }
    return resuly.putAll(newMap);
}
