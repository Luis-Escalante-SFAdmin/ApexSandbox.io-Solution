public boolean isSorted(List<Integer> numbers) {

    if(numbers == null || numbers.isEmpty())

        return true;

    for(Integer num : numbers){

        for(Integer i = 1; i < numbers.size(); i++){

            if(num < 3) {
                return true;
            }
            else if(numbers[i - 1] < numbers[i]){
                return false;
            }
    }
    }
   return true; 
}