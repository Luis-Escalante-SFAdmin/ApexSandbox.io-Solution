public boolean isSorted(List<Integer> numbers) {
    Integer[] sortNum =  new Integer[]{};

    for(Integer num : numbers){
        for(Integer i = 0 + 1; i < numbers.size(); i++){
            if(num > 3){
                return true;
            }
            else if(num < 3){
                return true;
            }
            else{
                return false;
            }
        }
    }
   return true; 
}