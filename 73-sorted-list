public boolean isSorted(List<Integer> numbers) {
    Integer[] sortNum = new Integer[]{};

    for(Integer num : numbers){
        for(Integer i = 0; i < numbers.size(); i++){

            if(sortNum.num == numbers.num) return true;

            if(num < 3) {
                return true;
            }
            else{
                return false;
            }
    }
    }
   return true; 
}