public boolean isSorted(List<Integer> numbers) {

    //if(numbers.size() <= 1) return true;

    for(Integer num : numbers){

        for(Integer i = 1; i < numbers.size(); i++){

            if(num < 3) {
                return true;
            }
            else if(numbers[i] < num){
                return false;
            }
    }
    }
   return true; 
}