public boolean isSorted(List<Integer> numbers) {
    numbers.sort();
    List<Integer> sortNum =  new List<Integer>();

    for(Integer num : numbers){
        for(Integer i = 0; i < numbers.size(); i++){
            if(num <= 3){
                return true;
            }
            if(sortNum.contains(num)){
                return true;
            }
    }
    }
   return true; 
}