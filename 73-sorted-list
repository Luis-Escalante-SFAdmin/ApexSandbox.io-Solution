public boolean isSorted(List<Integer> numbers) {
    Set<Integer> sortNum = new Set<Integer>();

    sortNum.contains(numbers);

    for(Integer num : numbers){
        for(Integer i = 0; i < numbers.size(); i++){
            if(num < 3) {
                return true;
            }
            else{
                return false;
            }
    }
    }
   return true; 
}