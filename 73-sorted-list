public boolean isSorted(List<Integer> numbers) {
    Set<Integer> sortNum = new Set<Integer>();


    for(Integer num : numbers){
        for(Integer i = 0; i < numbers.size() - 1; i++){

            if(num < 3) {
                return true;
            }
            else if(numbers[i] == 0 ){
                return false;
            }
            else if(numbers[i] == null){
                return false;
            }
    }
    }
   return true; 
}