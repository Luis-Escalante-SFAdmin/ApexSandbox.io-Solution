public boolean isSorted(List<Integer> numbers) {

    for(Integer num : numbers){

        for(Integer i = 0; i < numbers.size(); i++){

            if(num < 3) {
                return true;
            }
            else if(numbers[i] < num){
                return false;
            }
            else if(i == numbers.size() - 1){
                return false;
            }
    }
    }
   return true; 
}