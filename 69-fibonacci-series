public List<Integer> fibonacciSeries(Integer n) {
    Integer a = 1;
    Integer b = 1;
    Integer c;

    List<Integer> f = new List<Integer>();
    if(n < 2) return null;
    
    for(Integer fib : f)
    {
        if(n > 2)
        {
        c = a + b;
        a = b;
        b = c;
        f.add(fib);
        }
    }
    
    return f;
}