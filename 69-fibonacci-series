public List<Integer> fibonacciSeries(Integer n) {
    Integer a = 1;
    Integer b = 1;
    Integer c = 0;

    List<Integer> f = new List<Integer>();
    
    for(Integer i = 0; i < n; i++)
    {
        if(n > 2)
        {
        a = b;
        c = b + a;
        }
        f.add(c);
    }
    return f;
}