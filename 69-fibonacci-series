public List<Integer> fibonacciSeries(Integer n) {
    Integer a = 1;
    Integer b = 1;
    Integer c;

    if(n < 2) return null;
    
    for(Integer i = 2; i < n; i++)
    {
        List<Integer> f = new List<Integer>();
        c = a + b;
        a = b;
        b = c;
        f.add(c);
    }
    
    return f;
}