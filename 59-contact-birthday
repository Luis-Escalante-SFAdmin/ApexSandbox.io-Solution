public Boolean isBirthday(Contact c) {
    Date bDay = Date.today();
    Date notBday = Date.today();

    if(bDay == notBday)
    {
        return true;
    }
    else if(bDay != notBday)
    {
        return false;
    }
    else if(bDay > notBday)
    {
        return false;
    }
    return true;
}